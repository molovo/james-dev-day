{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "Slug"
      }
    },
    "page_title": {
      "type": "Text",
      "config": {
        "label": "Page Title"
      }
    }
  },
  "Meta": {
    "meta_title": {
      "type": "Text",
      "config": {
        "label": "Meta Title",
        "placeholder": "Page Meta Title"
      }
    },
    "meta_navigation_title": {
      "type": "Text",
      "config": {
        "label": "Meta Navigation Title",
        "placeholder": "Title for Navigation and Breadcrumbs"
      }
    },
    "meta_description": {
      "type": "Text",
      "config": {
        "label": "Meta Description",
        "placeholder": "Brief description of page content"
      }
    },
    "meta_robots": {
      "type": "Text",
      "config": {
        "label": "Meta Robots",
        "placeholder": "noindex, nofollow"
      }
    },
    "meta_image": {
      "type": "Image",
      "config": {
        "constraint": {},
        "thumbnails": [],
        "label": "Meta Image"
      }
    }
  },
  "Content": {
    "content": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
        "allowTargetBlank": true,
        "label": "Content"
      }
    }
  }
}
